{
  "layer": "top",
  "position": "top",
  "height": 20,

  "modules-left": [""],
  "modules-center": ["clock"],
  "modules-right": ["pulseaudio/slider"],

  "clock": {
    "interval": 1,
    "format": "{:%H:%M:%S}",
    "max-length": 25
  }

  "pulseaudio/slider": {
    "min": 0,
    "max": 100,
    "orientation": "horizontal"
  }
}
